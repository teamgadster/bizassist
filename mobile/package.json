{
	"name": "mobile",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"start:lan": "expo start",
		"start:clear": "expo start -c",
		"start:web": "expo start --web",
		"android:reverse": "adb start-server && adb wait-for-device && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:19000 tcp:19000 && adb reverse tcp:19001 tcp:19001",
		"android:reverse:list": "adb reverse --list",
		"android:kill": "adb shell am force-stop com.anonymous.mobile",
		"start:dev": "expo start --dev-client",
		"start:dev:clear": "expo start --dev-client -c",
		"start:android": "npm run android:reverse && expo start --dev-client --android --localhost",
		"start:android:clear": "npm run android:reverse && expo start --dev-client --android --localhost -c",
		"start:android:metro": "npm run android:reverse && npm run start:dev",
		"start:android:metro:clear": "npm run android:reverse && npm run start:dev:clear",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"android:bootstrap": "npx expo run:android && npm run start:android:metro",
		"ios:bootstrap": "npx expo run:ios && npm run start:dev",
		"metro:doctor": "node -e \"console.log('If Android shows 10.0.2.2:8081, clear Dev Settings > Debug server host & port, then force-stop the app and rerun start:android:metro')\"",
		"watchman:reset": "watchman watch-del '/Users/gerardogaden/Desktop/bizassist' ; watchman watch-project '/Users/gerardogaden/Desktop/bizassist'",
		"watchman:shutdown": "watchman shutdown-server",
		"reset-project": "node ./scripts/reset-project.js",
		"lint": "expo lint",
		"start:ios": "expo start --dev-client --ios --lan",
		"start:ios:clear": "expo start --dev-client --ios --lan -c"
	},
	"dependencies": {
		"@expo/vector-icons": "^15.0.3",
		"@gorhom/bottom-sheet": "^5.2.8",
		"@react-native-async-storage/async-storage": "2.2.0",
		"@react-navigation/bottom-tabs": "^7.10.0",
		"@react-navigation/elements": "^2.6.3",
		"@react-navigation/native": "^7.1.28",
		"@shopify/react-native-skia": "2.2.12",
		"@supabase/supabase-js": "^2.89.0",
		"@tanstack/react-query": "^5.90.12",
		"axios": "^1.13.2",
		"base64-arraybuffer": "^1.0.2",
		"expo": "~54.0.33",
		"expo-camera": "~17.0.10",
		"expo-constants": "~18.0.13",
		"expo-device": "~8.0.10",
		"expo-file-system": "~19.0.21",
		"expo-font": "~14.0.11",
		"expo-haptics": "~15.0.8",
		"expo-image": "~3.0.11",
		"expo-image-manipulator": "~14.0.8",
		"expo-image-picker": "~17.0.10",
		"expo-keep-awake": "~15.0.8",
		"expo-linking": "~8.0.11",
		"expo-media-library": "~18.2.1",
		"expo-modules-core": "~3.0.29",
		"expo-router": "~6.0.23",
		"expo-screen-orientation": "~9.0.8",
		"expo-splash-screen": "~31.0.13",
		"expo-status-bar": "~3.0.9",
		"expo-symbols": "~1.0.8",
		"expo-system-ui": "~6.0.9",
		"expo-web-browser": "~15.0.10",
		"js-tokens": "^10.0.0",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-hook-form": "^7.68.0",
		"react-native": "0.81.5",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-mmkv": "^4.1.1",
		"react-native-modalize": "^2.1.1",
		"react-native-nitro-modules": "^0.33.2",
		"react-native-paper": "^5.14.5",
		"react-native-portalize": "^1.0.7",
		"react-native-reanimated": "~4.1.1",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-svg": "15.12.1",
		"react-native-url-polyfill": "^3.0.0",
		"react-native-vector-icons": "^10.3.0",
		"react-native-web": "~0.21.0",
