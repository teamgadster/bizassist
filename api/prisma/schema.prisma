generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id                  String                @id @default(uuid())
  email               String                @unique @db.VarChar(120)
  rawEmail            String?               @db.VarChar(120)
  passwordHash        String                @db.VarChar(72)
  emailVerified       Boolean               @default(false)
  emailVerifiedAt     DateTime?
  firstName           String                @db.VarChar(50)
  middleName          String?               @db.VarChar(50)
  lastName            String                @db.VarChar(50)
  gender              Gender?
  phone               String?               @db.VarChar(40)
  birthDate           DateTime?
  avatarUrl           String?               @db.VarChar(500)
  avatarPublicId      String?               @db.VarChar(120)
  coverUrl            String?               @db.VarChar(500)
  coverPublicId       String?               @db.VarChar(120)
  role                UserRole              @default(OWNER)
  isActive            Boolean               @default(true)
  lastLoginAt         DateTime?
  lastLogoutAt        DateTime?
  tokenVersion        Int                   @default(1)
  activeBusinessId    String?
  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @updatedAt
  Address             Address[]
  AuditLog            AuditLog[]
  OwnedBusinesses     Business[]            @relation("UserOwnedBusinesses")
  CategoryVisibility  CategoryVisibility[]
  DiscountVisibility  DiscountVisibility[]
  EmailOtp            EmailOtp[]
  PasswordResetTicket PasswordResetTicket[]
  RefreshToken        RefreshToken[]
  StaffMembership     StaffMembership[]
  UnitVisibility      UnitVisibility[]
  ActiveBusiness      Business?             @relation("UserActiveBusiness", fields: [activeBusinessId], references: [id])

  @@index([createdAt])
  @@index([emailVerified, createdAt])
  @@index([activeBusinessId])
}

model Address {
  id         String      @id @default(uuid())
  type       AddressType
  line1      String      @db.VarChar(120)
  line2      String?     @db.VarChar(120)
  city       String      @db.VarChar(80)
  state      String?     @db.VarChar(80)
  postal     String?     @db.VarChar(20)
  country    String      @db.VarChar(2)
  phone      String?     @db.VarChar(40)
  label      String?     @db.VarChar(60)
  userId     String?
  businessId String?
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
  Business   Business?   @relation(fields: [businessId], references: [id], onDelete: Cascade)
  User       User?       @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([businessId, type])
  @@index([userId, type])
}

model AuditLog {
  id            String      @id @default(uuid())
  userId        String?
  action        AuditAction
  status        AuditStatus
  contextKey    String?     @db.VarChar(64)
  emailMasked   String?     @db.VarChar(120)
  reason        String?     @db.VarChar(255)
  ip            String?     @db.VarChar(45)
  userAgent     String?
  correlationId String?     @db.VarChar(80)
  createdAt     DateTime    @default(now())
  User          User?       @relation(fields: [userId], references: [id])

  @@index([userId, createdAt])
  @@index([action, createdAt])
  @@index([status, createdAt])
  @@index([contextKey, createdAt])
  @@index([createdAt])
}

model Business {
  id                     String                  @id @default(uuid())
  name                   String                  @db.VarChar(120)
  businessType           BusinessType
  countryCode            String                  @db.VarChar(2)
  currencyCode           String                  @db.VarChar(3)
  timezone               String                  @db.VarChar(64)
  logoUrl                String?                 @db.VarChar(500)
  logoPublicId           String?                 @db.VarChar(120)
  coverUrl               String?                 @db.VarChar(500)
  coverPublicId          String?                 @db.VarChar(120)
  description            String?                 @db.VarChar(500)
  foundedAt              DateTime?
  ownerId                String
  settings               Json?
  createdAt              DateTime                @default(now())
  updatedAt              DateTime                @updatedAt
  Address                Address[]
  Owner                  User                    @relation("UserOwnedBusinesses", fields: [ownerId], references: [id], onDelete: Cascade)
  Counter                BusinessCounter?
  Categories             Category[]
  CategoryVisibility     CategoryVisibility[]
  Devices                Device[]
  Discounts              Discount[]
  DiscountVisibility     DiscountVisibility[]
  InventoryMovements     InventoryMovement[]
  ModifierGroups         ModifierGroup[]
  ProductModifierGroups  ProductModifierGroup[]
  ModifierOptions        ModifierOption[]
  SaleLineItemModifiers  SaleLineItemModifier[]
  Products               Product[]
  ProductImages          ProductImage[]
  Sales                  Sale[]
  SaleDiscount           SaleDiscount[]
  StaffMembership        StaffMembership[]
  Stores                 Store[]
  Units                  Unit[]
  UnitVisibility         UnitVisibility[]
  ActiveUsers            User[]                  @relation("UserActiveBusiness")

  @@index([businessType, countryCode])
  @@index([ownerId])
  @@index([createdAt])
  @@index([countryCode])
  @@index([currencyCode])
}

model Store {
  id                 String              @id @default(uuid())
  businessId         String
  name               String              @db.VarChar(120)
  code               String?             @db.VarChar(32)
  isDefault          Boolean             @default(false)
  address            Json?
  isActive           Boolean             @default(true)
  createdAt          DateTime            @default(now())
  updatedAt          DateTime            @updatedAt
  InventoryMovements InventoryMovement[]
  Products           Product[]
  Business           Business            @relation(fields: [businessId], references: [id], onDelete: Cascade)

  @@index([businessId, isDefault])
  @@index([businessId, createdAt])
}

model Product {
  id                        String              @id @default(uuid())
  businessId                String
  storeId                   String?
  type                      ProductType         @default(PHYSICAL)
  name                      String              @db.VarChar(120)
  sku                       String              @db.VarChar(64)
  barcode                   String?             @db.VarChar(64)
  description               String?             @db.VarChar(500)
  price                     Decimal?            @db.Decimal(12, 2)
  priceMinor                BigInt?
  cost                      Decimal?            @db.Decimal(12, 2)
  costMinor                 BigInt?
  trackInventory            Boolean             @default(true)
  reorderPoint              Decimal?            @db.Decimal(18, 5)
  onHandCached              Decimal             @default(0) @db.Decimal(18, 5)
  primaryImageUrl           String?             @db.VarChar(500)
  isActive                  Boolean             @default(true)
  createdAt                 DateTime            @default(now())
  updatedAt                 DateTime            @updatedAt
  categoryId                String?
  categoryLegacy            String?             @db.VarChar(80)
  unitId                    String?
  posTileColor              String?             @db.VarChar(16)
  posTileLabel              String?             @db.VarChar(5)
  posTileMode               PosTileMode         @default(COLOR)
  serviceDurationMins       Int?
  serviceUnitLabel          String?             @db.VarChar(40)
  serviceUnitPrecision      Int?
  durationTotalMinutes      Int?
  processingEnabled         Boolean             @default(false)
  durationInitialMinutes    Int?
  durationProcessingMinutes Int?
  durationFinalMinutes      Int?
  movements                 InventoryMovement[]
  Business                  Business            @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Category                  Category?           @relation(fields: [categoryId], references: [id])
  Store                     Store?              @relation(fields: [storeId], references: [id])
  Unit                      Unit?               @relation(fields: [unitId], references: [id], onDelete: SetNull)
  images                    ProductImage[]
  productModifierGroups     ProductModifierGroup[]
  lineItems                 SaleLineItem[]

  @@unique([businessId, sku])
  @@unique([businessId, barcode])
  @@index([businessId, createdAt])
  @@index([businessId, name])
  @@index([businessId, type])
  @@index([businessId, type, isActive])
  @@index([storeId])
  @@index([businessId, categoryId])
  @@index([businessId, unitId])
}

model BusinessCounter {
  id                   String   @id @default(uuid())
  businessId           String   @unique
  nextProductSkuNumber Int      @default(1)
  createdAt            DateTime @default(now())
  updatedAt            DateTime @updatedAt
  Business             Business @relation(fields: [businessId], references: [id], onDelete: Cascade)

  @@index([businessId])
}

model UnitCatalog {
  id             String       @id @default(uuid())
  category       UnitCategory
  name           String       @db.VarChar(80)
  abbreviation   String       @db.VarChar(16)
  precisionScale Int          @default(0)
  isActive       Boolean      @default(true)
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt
  enabledUnits   Unit[]

  @@unique([category, name])
  @@index([isActive, category, name])
  @@index([createdAt])
}

model Unit {
  id             String           @id @default(uuid())
  businessId     String
  source         UnitSource       @default(CUSTOM)
  catalogId      String?
  name           String           @db.VarChar(80)
  abbreviation   String           @db.VarChar(16)
  category       UnitCategory
  precisionScale Int              @default(0)
  isActive       Boolean          @default(true)
  createdAt      DateTime         @default(now())
  updatedAt      DateTime         @updatedAt
  nameNormalized String           @db.VarChar(80)
  Products       Product[]
  Business       Business         @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Catalog        UnitCatalog?     @relation(fields: [catalogId], references: [id])
  UnitVisibility UnitVisibility[]

  @@unique([businessId, name])
  @@unique([businessId, nameNormalized])
  @@unique([businessId, catalogId])
  @@index([businessId, isActive])
  @@index([businessId, category, name])
  @@index([catalogId])
  @@index([createdAt])
}

model UnitVisibility {
  id         String   @id @default(uuid())
  userId     String
  businessId String
  unitId     String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  Business   Business @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Unit       Unit     @relation(fields: [unitId], references: [id], onDelete: Cascade)
  User       User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, businessId, unitId])
  @@index([businessId, userId])
  @@index([unitId])
  @@index([createdAt])
}

model CategoryVisibility {
  id         String   @id @default(uuid())
  userId     String
  businessId String
  categoryId String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  Business   Business @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Category   Category @relation(fields: [categoryId], references: [id], onDelete: Cascade)
  User       User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, businessId, categoryId])
  @@index([businessId, userId])
  @@index([categoryId])
  @@index([createdAt])
}

model DiscountVisibility {
  id         String   @id @default(uuid())
  userId     String
  businessId String
  discountId String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  Business   Business @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Discount   Discount @relation(fields: [discountId], references: [id], onDelete: Cascade)
  User       User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, businessId, discountId])
  @@index([businessId, userId])
  @@index([discountId])
  @@index([createdAt])
}

model Category {
  id                 String               @id @default(uuid())
  businessId         String
  name               String               @db.VarChar(80)
  color              String?              @db.VarChar(16)
  sortOrder          Int                  @default(0)
  isActive           Boolean              @default(true)
  archivedAt         DateTime?
  createdAt          DateTime             @default(now())
  updatedAt          DateTime             @updatedAt
  nameNormalized     String               @db.VarChar(80)
  Business           Business             @relation(fields: [businessId], references: [id], onDelete: Cascade)
  CategoryVisibility CategoryVisibility[]
  Products           Product[]

  @@unique([businessId, name])
  @@unique([businessId, nameNormalized])
  @@index([businessId, archivedAt, sortOrder])
  @@index([businessId, isActive, sortOrder])
  @@index([businessId, createdAt])
}

model ProductImage {
  id           String             @id @default(uuid())
  businessId   String
  productId    String
  kind         ProductImageKind   @default(PRIMARY_POS_TILE)
  provider     StorageProvider    @default(SUPABASE)
  bucket       String             @db.VarChar(80)
  path         String             @db.VarChar(300)
  publicUrl    String             @db.VarChar(500)
  processedUrl String?            @db.VarChar(500)
  status       ProductImageStatus @default(ORIGINAL)
  mimeType     String?            @db.VarChar(80)
  bytes        Int?
  width        Int?
  height       Int?
  isPrimary    Boolean            @default(false)
  sortOrder    Int                @default(0)
  createdAt    DateTime           @default(now())
  updatedAt    DateTime           @updatedAt
  Business     Business           @relation(fields: [businessId], references: [id], onDelete: Cascade)
  product      Product            @relation(fields: [productId], references: [id], onDelete: Cascade)

  @@index([businessId, productId, kind, sortOrder])
  @@index([productId])
}

model Discount {
  id                 String               @id @default(uuid())
  businessId         String
  name               String               @db.VarChar(120)
  type               DiscountType
  value              Decimal              @db.Decimal(12, 2)
  valueMinor         BigInt?
  isActive           Boolean              @default(true)
  createdAt          DateTime             @default(now())
  updatedAt          DateTime             @updatedAt
  isStackable        Boolean              @default(false)
  archivedAt         DateTime?
  description        String?              @db.VarChar(250)
  nameNormalized     String               @db.VarChar(120)
  Business           Business             @relation(fields: [businessId], references: [id], onDelete: Cascade)
  DiscountVisibility DiscountVisibility[]
  SaleDiscount       SaleDiscount[]

  @@unique([businessId, nameNormalized])
  @@index([businessId, archivedAt])
  @@index([businessId, createdAt])
  @@index([businessId, isActive])
  @@index([businessId, name])
}

model ModifierGroup {
  id            String                  @id @default(uuid())
  businessId    String
  name          String                  @db.VarChar(120)
  selectionType ModifierSelectionType
  isRequired    Boolean                 @default(false)
  minSelected   Int                     @default(0)
  maxSelected   Int                     @default(1)
  sortOrder     Int                     @default(0)
  isArchived    Boolean                 @default(false)
  createdAt     DateTime                @default(now())
  updatedAt     DateTime                @updatedAt
  Business      Business                @relation(fields: [businessId], references: [id], onDelete: Cascade)
  productLinks  ProductModifierGroup[]
  options       ModifierOption[]

  @@index([businessId, isArchived, sortOrder])
  @@index([businessId, createdAt])
}

model ProductModifierGroup {
  id              String        @id @default(uuid())
  businessId      String
  productId       String
  modifierGroupId String
  sortOrder       Int           @default(0)
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @updatedAt
  Business        Business      @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Product         Product       @relation(fields: [productId], references: [id], onDelete: Cascade)
  ModifierGroup   ModifierGroup @relation(fields: [modifierGroupId], references: [id], onDelete: Cascade)

  @@unique([productId, modifierGroupId])
  @@index([businessId, productId, sortOrder])
  @@index([businessId, modifierGroupId])
}

model ModifierOption {
  id                    String                 @id @default(uuid())
  businessId            String
  modifierGroupId       String
  name                  String                 @db.VarChar(120)
  priceDeltaMinor       BigInt                 @default(0)
  sortOrder             Int                    @default(0)
  isSoldOut             Boolean                @default(false)
  isArchived            Boolean                @default(false)
  createdAt             DateTime               @default(now())
  updatedAt             DateTime               @updatedAt
  Business              Business               @relation(fields: [businessId], references: [id], onDelete: Cascade)
  ModifierGroup         ModifierGroup          @relation(fields: [modifierGroupId], references: [id], onDelete: Cascade)
  SaleLineItemModifiers SaleLineItemModifier[]

  @@index([businessId, modifierGroupId, isArchived, sortOrder])
  @@index([businessId, createdAt])
}

model Sale {
  id                 String              @id @default(cuid())
  businessId         String
  deviceId           String?
  idempotencyKey     String
  subtotal           Decimal             @db.Decimal(12, 2)
  subtotalMinor      BigInt?
  total              Decimal             @db.Decimal(12, 2)
  totalMinor         BigInt?
  discountTotal      Decimal             @default(0) @db.Decimal(12, 2)
  discountTotalMinor BigInt?
  taxTotal           Decimal             @default(0) @db.Decimal(12, 2)
  taxTotalMinor      BigInt?
  status             SaleStatus          @default(COMPLETED)
  createdAt          DateTime            @default(now())
  movements          InventoryMovement[]
  payments           Payment[]
  Business           Business            @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Device             Device?             @relation(fields: [deviceId], references: [id])
  discounts          SaleDiscount[]
  lineItems          SaleLineItem[]

  @@unique([businessId, idempotencyKey])
  @@index([businessId, createdAt])
  @@index([deviceId])
}

model SaleLineItem {
  id          String         @id @default(cuid())
  saleId      String
  productId   String
  productName String
  quantity    Int
  quantityV2  Decimal?       @db.Decimal(18, 5)
  unitPrice   Decimal        @db.Decimal(12, 2)
  lineTotal   Decimal        @db.Decimal(12, 2)
  selectedModifierOptionIds Json?
  totalModifiersDeltaMinor  BigInt?
  discounts   SaleDiscount[]
  modifiers   SaleLineItemModifier[]
  product     Product        @relation(fields: [productId], references: [id])
  sale        Sale           @relation(fields: [saleId], references: [id], onDelete: Cascade)

  @@index([saleId])
  @@index([productId])
}

model SaleLineItemModifier {
  id               String         @id @default(cuid())
  businessId       String
  saleLineItemId   String
  modifierOptionId String
  optionName       String         @db.VarChar(120)
  priceDeltaMinor  BigInt
  createdAt        DateTime       @default(now())
  Business         Business       @relation(fields: [businessId], references: [id], onDelete: Cascade)
  SaleLineItem     SaleLineItem   @relation(fields: [saleLineItemId], references: [id], onDelete: Cascade)
  ModifierOption   ModifierOption @relation(fields: [modifierOptionId], references: [id], onDelete: Restrict)

  @@index([businessId, saleLineItemId])
  @@index([businessId, modifierOptionId])
}

model SaleDiscount {
  id                 String        @id @default(cuid())
  businessId         String
  saleId             String
  discountId         String?
  scope              DiscountScope
  saleLineItemId     String?
  nameSnapshot       String        @db.VarChar(120)
  typeSnapshot       DiscountType
  valueSnapshot      Decimal       @db.Decimal(12, 2)
  valueSnapshotMinor BigInt?
  amountApplied      Decimal       @db.Decimal(12, 2)
  amountAppliedMinor BigInt?
  createdAt          DateTime      @default(now())
  Business           Business      @relation(fields: [businessId], references: [id], onDelete: Cascade)
  discount           Discount?     @relation(fields: [discountId], references: [id])
  Sale               Sale          @relation(fields: [saleId], references: [id], onDelete: Cascade)
  SaleLineItem       SaleLineItem? @relation(fields: [saleLineItemId], references: [id], onDelete: Cascade)

  @@index([businessId, saleId])
  @@index([saleId])
  @@index([saleLineItemId])
  @@index([discountId])
}

model Payment {
  id          String        @id @default(cuid())
  saleId      String
  method      PaymentMethod
  amount      Decimal       @db.Decimal(12, 2)
  amountMinor BigInt?
  sale        Sale          @relation(fields: [saleId], references: [id], onDelete: Cascade)

  @@index([saleId])
}

model InventoryMovement {
  id                 String                  @id @default(cuid())
  businessId         String
  productId          String
  storeId            String?
  quantityDelta      Decimal                 @db.Decimal(18, 5)
  reason             InventoryMovementReason
  idempotencyKey     String?                 @db.VarChar(80)
  relatedSaleId      String?
  createdAt          DateTime                @default(now())
  Business           Business                @relation(fields: [businessId], references: [id], onDelete: Cascade)
  Product            Product                 @relation(fields: [productId], references: [id], onDelete: Cascade)
  RelatedSale        Sale?                   @relation(fields: [relatedSaleId], references: [id])
  Store              Store?                  @relation(fields: [storeId], references: [id])

  @@unique([businessId, idempotencyKey])
  @@index([businessId, productId, createdAt])
  @@index([storeId])
  @@index([relatedSaleId])
}

model Device {
  id         String   @id @default(cuid())
  businessId String
  name       String   @db.VarChar(80)
  createdAt  DateTime @default(now())
  Business   Business @relation(fields: [businessId], references: [id], onDelete: Cascade)
  sales      Sale[]

  @@index([businessId])
}

model EmailOtp {
  id         String     @id @default(uuid())
  userId     String
  email      String     @db.VarChar(120)
  codeHash   String     @db.VarChar(128)
  purpose    OtpPurpose
  expiresAt  DateTime
  attempts   Int        @default(0)
  lastSentAt DateTime   @default(now())
  createdAt  DateTime   @default(now())
  updatedAt  DateTime   @updatedAt
  User       User       @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, purpose])
  @@index([createdAt])
  @@index([email, purpose, expiresAt])
  @@index([email, purpose, lastSentAt])
  @@index([expiresAt])
}

model RefreshToken {
  id        String    @id @default(uuid())
  userId    String
  tokenHash String    @db.VarChar(255)
  expiresAt DateTime
  revokedAt DateTime?
  userAgent String?
  ip        String?   @db.VarChar(45)
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  User      User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, tokenHash])
  @@index([expiresAt])
  @@index([userId])
}

model StaffMembership {
  id         String    @id @default(uuid())
  userId     String
  businessId String
  staffRole  StaffRole
  isPrimary  Boolean   @default(false)
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
  Business   Business  @relation(fields: [businessId], references: [id], onDelete: Cascade)
  User       User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, businessId])
  @@index([businessId, staffRole])
  @@index([userId, staffRole])
}

model PasswordResetTicket {
  id        String    @id @default(uuid())
  userId    String
  tokenHash String    @db.VarChar(128)
  expiresAt DateTime
  usedAt    DateTime?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  User      User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, tokenHash])
  @@index([tokenHash])
  @@index([expiresAt])
  @@index([userId, createdAt])
}

enum UnitCategory {
  COUNT
  WEIGHT
  VOLUME
  LENGTH
  AREA
  TIME
  CUSTOM
}

enum UnitSource {
  CATALOG
  CUSTOM
}

enum SaleStatus {
  COMPLETED
}

enum PaymentMethod {
  CASH
  EWALLET
  BANK_TRANSFER
  OTHER
}

enum InventoryMovementReason {
  SALE
  STOCK_IN
  STOCK_OUT
  ADJUSTMENT
}

enum ModifierSelectionType {
  SINGLE
  MULTI
}

enum ProductType {
  PHYSICAL
  SERVICE
}

enum DiscountType {
  PERCENT
  FIXED
}

enum DiscountScope {
  SALE
  LINE_ITEM
}

enum PosTileMode {
  IMAGE
  COLOR
}

enum StorageProvider {
  SUPABASE
}

enum ProductImageStatus {
  ORIGINAL
  BACKGROUND_REMOVED
  FAILED
}

enum ProductImageKind {
  PRIMARY_POS_TILE
  SECONDARY_GALLERY
  LIFESTYLE
}

enum UserRole {
  OWNER
  ADMIN
  STAFF
}

enum StaffRole {
  OWNER
  MANAGER
  CASHIER
  STAFF
}

enum OtpPurpose {
  REGISTER
  PASSWORD_RESET
  CHANGE_EMAIL
}

enum AddressType {
  BUSINESS_PRIMARY
  BUSINESS_BRANCH
  OWNER_HOME
  OWNER_BILLING
  OTHER
}

enum AuditAction {
  AUTH_REGISTER
  AUTH_LOGIN
  AUTH_LOGOUT
  PASSWORD_RESET
  USER_EMAIL_CHANGE
  BUSINESS_CREATE
  RATE_LIMIT
}

enum AuditStatus {
  SUCCESS
  FAIL
  DENIED
}

enum BusinessType {
  GENERAL_RETAIL
  CLINIC
  VET
  GROOMING
  RESTAURANT
  OTHER
}

enum Gender {
  MALE
  FEMALE
  OTHERS
}
