		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.19.3",
		"@types/pg": "^8.15.6",
		"@types/supertest": "^6.0.3",
		"jest": "^29.7.0",
		"nodemon": "^3.1.11",
		"prisma": "^7.3.0",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
