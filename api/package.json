		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"countries-and-timezones": "^3.8.0",
		"country-to-currency": "^2.0.2",
		"dotenv": "^16.4.0",
		"express": "^4.21.0",
		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^8.2.1",
		"helmet": "^8.1.0",
		"http-status-codes": "^2.3.0",
